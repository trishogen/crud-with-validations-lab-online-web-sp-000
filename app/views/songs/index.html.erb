<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Released</th>
      <th>Release Year</th>
      <th>Genre</th>
      <th>Artist Name</th>
      <th>Delete Button</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td><%= song.artist_name %></td>
        <td><%= link_to "Delete", song/delete, method: :delete, data: { confirm: "Really?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
